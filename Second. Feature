               
			   
			   Count the words in String
            System.out.println("enter the string");
            Scanner sc =new Scanner(System.in);
		    String  s =sc.nextline();
			int count = 1;
			for(int i=0;i<=s.length-1;i++){
			if((s.charAt(i)==' '&& s.charAt(i)!=' '))
			{
			 count = ++;
			 
			}
			System.out.println(count);